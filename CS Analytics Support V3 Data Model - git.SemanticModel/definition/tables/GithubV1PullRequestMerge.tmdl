	partition GithubV1PullRequestMerge = m
		mode: directQuery
		source =
				let
				    Source = AzureDataExplorer.Contents("gh-analytics.eastus.kusto.windows.net", "hydro", " github_v1_pull_request_merge#(lf)    | extend username = tostring(parse_json(author).login)#(lf)    | extend JsonData = parse_json(pull_request)#(lf)    | extend supportPath = tostring(JsonData.changed_files[0].current_path)#(lf)    | extend changetype = tostring(JsonData.changed_files[0].change_type)#(lf)    | extend statustype = tostring(JsonData.changed_files[0].change_type)#(lf)    | extend repoid = tostring(parse_json(repository).id)#(lf)    | where repoid in (""245519314"", ""2832707"")#(lf)    | extend weekdate = bin(timestamp, 7d)#(lf)    | extend monthdate = bin(timestamp, 30d)#(lf)    | extend containsSupport = supportPath contains ""support""#(lf)    | extend containsMD = supportPath contains "".md""#(lf)    | extend containsIndex = supportPath contains ""index""#(lf)    | extend containsBot = supportPath contains ""[bot]""#(lf)    | extend filePath2 = tostring(substring(supportPath, strlen(""docs/support/""))) // cut off the front useless bit#(lf)    | extend articleList = trim_end("".md"", filePath2) // Remove file extension early#(lf)    | extend articleList = trim_end(""_index"", articleList) // Remove _index suffix early#(lf)    | extend articleList = trim_end(""/"", articleList)#(lf)|extend DateModified = format_datetime(timestamp, 'yyyy-MM-dd'), changetype, FilePath = tostring(substring(supportPath, strlen(""docs/support/"")))", [MaxRows=null, MaxSize=null, NoTruncate=null, AdditionalSetStatements=null])
				in
				    Source

	annotation PBI_ResultType = Exception

